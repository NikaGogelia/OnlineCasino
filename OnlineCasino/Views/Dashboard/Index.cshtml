@{
	ViewData["Title"] = "Admin Dashboard";
}


<head>
	<link href="https://cdn.datatables.net/2.2.1/css/dataTables.dataTables.min.css" rel="stylesheet" />
</head>

<div class="dashboard">
	<h1>Admin Dashboard</h1>

	<table id="DashboardTable" class="display" style="width:100%">
		<thead>
			<tr>
				<th>Transaction Id</th>
				<th>User Name</th>
				<th>Transaction Type</th>
				<th>Amount</th>
				<th>Transaction Status</th>
				<th>Transaction Approve/Reject</th>
			</tr>
		</thead>
		<tbody>
		</tbody>
	</table>
</div>

<script src="https://cdn.datatables.net/2.2.1/js/dataTables.min.js"></script>
<script src="~/js/AdminDashboard.js" asp-append-version="true"></script>
