@{
	ViewData["Title"] = "Transactions History";
}


<head>
	<link href="https://cdn.datatables.net/2.2.1/css/dataTables.dataTables.min.css" rel="stylesheet" />
</head>

<div class="transactions-history">
	<h1 class="mb-4 text-primary">Transactions History</h1>

	<table id="TransactionsTable" class="display" style="width:100%">
		<thead>
			<tr>
				<th>Transaction Id</th>
				<th>User Name</th>
				<th>Amount</th>
				<th>Transaction Status</th>
				<th>Creation Time</th>
			</tr>
		</thead>
		<tbody>
		</tbody>
	</table>
</div>

<script src="~/js/TransactionsHistory.js" asp-append-version="true"></script>
